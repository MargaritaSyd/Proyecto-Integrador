<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>MaMeLi - Panel administrador de productos</title>
        <link rel="stylesheet" href="/css/productForm.css">
        <link rel="stylesheet" href="/css/shop.css">
        <link rel="stylesheet" href="/css/login.css">
        <link rel="stylesheet" href="/css/panel.css">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <link href="https://fonts.googleapis.com/css?family=Droid+Sans:400,700" rel="stylesheet">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.8.1/baguetteBox.min.css">
        <link rel="stylesheet" href="gallery-grid.css">
        <link rel="stylesheet" href="/css/indexstylesheet.css">
        <link href="styles/ihover.css" rel="stylesheet">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
    </head>

    <body>
        <div class="fixed-top">
            <header class="newHeader">
                <div class="container">
                    <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                        <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
                            <svg class="bi me-2" width="40" height="32" role="img" aria-label="Bootstrap"><use xlink:href="#bootstrap"/></svg>
                        </a>
                        <span>
                            <a href="/"> <img class="logo" src="/imagenes/logo.png" alt="logo"></a> 
                        </span>
                        <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                            <li><a href="/" class="nav-link px-2 text-white botonHeader">Inicio</a></li>
                            <li><a href="/users/login" class="nav-link px-2 text-white botonHeader">Perfil</a></li>
                            <li><a href="/product" class="nav-link px-2 text-white botonHeader">Productos</a></li>
                            <li><a href="/faqs" class="nav-link px-2 text-white botonHeader">FAQs</a></li>
                            <li><a href="/nosotros" class="nav-link px-2 text-white botonHeader">Nosotros</a></li>
                            <li><a href="/cart" <i class="fas fa-shopping-cart botonHeader"></i></a></li>
                        </ul>
                        <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
                            <input type="search" class="form-control form-control-dark" placeholder="Buscar producto.." aria-label="Search">
                        </form>
                        <div class="text-end">
                            <a href="/users/login" <button type="button" class="btn btn-outline-light me-2" >Ingresá!</button> </a>
                            <a href="/users/register" <button type="button" class="btn btn-warning" >Creá tu cuenta</button> </a>
                        </div>
                    </div>
                </div>
            </header>
        </div>
        <div class="space"></div>
        <div class="space"></div>
        <div class="space"></div>
        <div class="space"></div>
        <div class="space" ></div>
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb" style="margin-bottom: 0;">
                <!-- <li class="breadcrumb-item breadmenu"><a href="/">Home</a></li>
                <li class="breadcrumb-item active breadmenu" aria-current="page">Products</li> -->
            </ol>
        </nav>
        <main>
            <nav class="panel-top_nav">
                <ul class="panel-top_nav_ul" type="none">
                    <li><a class="panel-top_nav_ul_li" href="#accesorios">Accesorios</a></li>
                    <li><a class="panel-top_nav_ul_li" href="#combos">Combos</a></li>
                    <li><a class="panel-top_nav_ul_li" href="#macetas">Macetas</a></li>
                    <li><a class="panel-top_nav_ul_li" href="#plantas">Plantas</a></li>
                    <li><a class="panel-top_nav_ul_li" href="#terrarios">Terrarios</a></li>
                    <li><a class="panel-top_nav_ul_li new_product" href="/product/create">Nuevo producto</a></li>
                </ul>
            </nav>
            <div id="accesorios"></div>
            <div class="div_under_nav"></div>

            <!-- Accesorios -->
            <section>
                <h2 class="panel-section-h2">Accesorios</h2>
                <div class="panel_section_div">
                    <% accesorios.forEach(product => { %>
                        <article class="text_align_center">
                            <% if (product.showing==1) { %>
                                <h3 class="panel_section_div_article_h3"><%=product.name%><br><small class="showing_small_msg">(en exhibición)</small></h3>
                                <div class="div_container_image">
                                    <img class="image" src="/imagenes/productImages/<%=product.image%>"> 
                                </div>
                            <% } else { %>
                                <h3 class="panel_section_div_article_h3 noshowing"><%=product.name%><br><small class="noshowing no_showing_small_msg">(no exhibido)</small></h3>
                                <div class="div_container_image">
                                    <img class="image image_no_showing" src="/imagenes/productImages/<%=product.image%>"> 
                                </div>
                            <% } %>   
                            <a href="/product/edit/<%=product.id%>"><button class="edit_product_button">Editar producto</button></a> 
                        </article>
                    <% }) %>
                    <div id="combos"></div>
                </div>
            </section>
            <!-- end of accesorios -->

            <!-- Combos -->
            <section>
                <h2 class="panel-section-h2">Combos</h2>
                <div class="panel_section_div">
                    <% combos.forEach(product => { %>
                        <article class="text_align_center">
                            <% if (product.showing==1) { %>
                                <h3 class="panel_section_div_article_h3"><%=product.name%><br><small class="showing_small_msg">(en exhibición)</small></h3>
                                <div class="div_container_image">
                                    <img class="image" src="/imagenes/productImages/<%=product.image%>"> 
                                </div>
                            <% } else { %>
                                <h3 class="panel_section_div_article_h3 noshowing"><%=product.name%><br><small class="noshowing no_showing_small_msg">(no exhibido)</small></h3>
                                <div class="div_container_image">
                                    <img class="image image_no_showing" src="/imagenes/productImages/<%=product.image%>"> 
                                </div>
                            <% } %>   
                            <a href="/product/edit/<%=product.id%>"><button class="edit_product_button">Editar producto</button></a> 
                        </article>
                    <% }) %>
                    <div id="macetas"></div>
                </div>
            </section>
            <!-- end of combos -->

            <!-- Macetas -->
            <section>
                <h2 class="panel-section-h2">Macetas</h2>
                <div class="panel_section_div">
                    <% macetas.forEach(product => { %>
                        <article class="text_align_center">
                            <% if (product.showing==1) { %>
                                <h3 class="panel_section_div_article_h3"><%=product.name%><br><small class="showing_small_msg">(en exhibición)</small></h3>
                                <div class="div_container_image">
                                    <img class="image" src="/imagenes/productImages/<%=product.image%>"> 
                                </div>
                            <% } else { %>
                                <h3 class="panel_section_div_article_h3 noshowing"><%=product.name%><br><small class="noshowing no_showing_small_msg">(no exhibido)</small></h3>
                                <div class="div_container_image">
                                    <img class="image image_no_showing" src="/imagenes/productImages/<%=product.image%>"> 
                                </div>
                            <% } %>   
                            <a href="/product/edit/<%=product.id%>"><button class="edit_product_button">Editar producto</button></a> 
                        </article>
                    <% }) %>
                    <div id="plantas"></div>
                </div>
            </section>
            <!-- end of Macetas -->

            <!-- Plantas -->
            <section>
                <h2 class="panel-section-h2">Plantas</h2>
                <div class="panel_section_div">
                    <% plantas.forEach(product => { %>
                        <article class="text_align_center">
                            <% if (product.showing==1) { %>
                                <h3 class="panel_section_div_article_h3"><%=product.name%><br><small class="showing_small_msg">(en exhibición)</small></h3>
                                <div class="div_container_image">
                                    <img class="image" src="/imagenes/productImages/<%=product.image%>"> 
                                </div>
                            <% } else { %>
                                <h3 class="panel_section_div_article_h3 noshowing"><%=product.name%><br><small class="noshowing no_showing_small_msg">(no exhibido)</small></h3>
                                <div class="div_container_image">
                                    <img class="image image_no_showing" src="/imagenes/productImages/<%=product.image%>"> 
                                </div>
                            <% } %>   
                            <a href="/product/edit/<%=product.id%>"><button class="edit_product_button">Editar producto</button></a> 
                        </article>
                    <% }) %>
                    <div id="terrarios"></div>
                </div>
            </section>
            <!-- end of Plantas -->

            <!-- Terrarios -->
            <section class="section_terrarios">
                <h2 class="panel-section-h2">Terrarios</h2>
                <div class="panel_section_div">
                    <% terrarios.forEach(product => { %>
                        <article class="text_align_center">
                            <% if (product.showing==1) { %>
                                <h3 class="panel_section_div_article_h3"><%=product.name%><br><small class="showing_small_msg">(en exhibición)</small></h3>
                                <div class="div_container_image">
                                    <img class="image" src="/imagenes/productImages/<%=product.image%>"> 
                                </div>
                            <% } else { %>
                                <h3 class="panel_section_div_article_h3 noshowing"><%=product.name%><br><small class="noshowing no_showing_small_msg">(no exhibido)</small></h3>
                                <div class="div_container_image">
                                    <img class="image image_no_showing" src="/imagenes/productImages/<%=product.image%>"> 
                                </div>
                            <% } %>   
                            <a href="/product/edit/<%=product.id%>"><button class="edit_product_button">Editar producto</button></a> 
                        </article>
                    <% }) %>
                </div>
            </section>
            <!-- end of Terrarios -->

        </main>        

        <%- include('../partials/footer') %>
        <script src="https://kit.fontawesome.com/17464b966e.js" crossorigin="anonymous"></script>
        <!-- <script src="/javaScript/cart.js"></script>   -->
    </body>
</html>